{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "$schema": "../gen/schemas/desktop-schema.json",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "log:default",
    "clipboard-manager:allow-write-text",
    "fs:read-files",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "/"
        },
        {
          "path": "/**"
        }
      ]
    },
    "clipboard-manager:allow-write-image"
  ]
}